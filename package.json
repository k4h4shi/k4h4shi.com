{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "hexo server",
    "browser": "opener http://localhost:4000",
    "start": "npm-run-all -p server browser",
    "prestart": "npm run build",
    "build": "hexo generate",
    "create": "hexo new",
    "deploy": "hexo deploy -g"
  },
  "hexo": {
    "version": "3.3.9"
  },
  "dependencies": {
    "hexo": "^3.2.0",
    "hexo-deployer-git": "^0.3.1",
    "hexo-generator-archive": "^0.1.4",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-pagination": "^0.1.0",
    "hexo-renderer-ejs": "^0.3.0",
    "hexo-renderer-marked": "^0.3.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.1",
    "opener": "^1.4.3"
  }
}
